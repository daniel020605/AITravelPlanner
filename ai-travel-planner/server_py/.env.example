# Copy to .env and fill your Supabase Postgres connection string
DATABASE_URL=postgresql://postgres:YOUR-PASSWORD@db.xxxxx.supabase.co:5432/postgres
# Optional: change port with UVICORN_PORT
UVICORN_PORT=8000

# Security
# Requests must include header "X-API-KEY: <SYNC_API_KEY>" if set.
SYNC_API_KEY=please-change-me

# CORS origins whitelist (comma separated). Use "*" for all.
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Optional client IP whitelist (comma separated). Leave empty to disable.
ALLOWED_IPS=